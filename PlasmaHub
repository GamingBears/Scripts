-- EXPERIMENTAL TAB

local TestTab = Window:CreateTab("Test Tab", 4483362458) -- Title, Image
local TestSection = TestTab:CreateSection("Features","")

local Button = TestTab:CreateButton({
	Name = "Print Test",
	Callback = function()
		print("Execution Working")
	end,
})

local Button = TestTab:CreateButton({
	Name = "Window Prompt",
	Callback = function()
Window:Prompt({
    Title = 'Title Man:',
    SubTitle = 'Subtitle',
    Content = 'Very good GUI but for whatever reason the accept button is goofy',
    Actions = {
        Accept = {
            Name = 'Accept',
            Callback = function()
                print('Pressed')
            end,
        }
    }
})
	end,
})

local Button = TestTab:CreateButton({
	Name = "Notify",
	Callback = function()
		ArrayField:Notify({
			Title = "Notification Title",
			Content = "Notification Content",
			Duration = 6.5,
			Image = 4483362458,
			Actions = { -- Notification Buttons
				Ignore = {
					Name = "Okay!",
					Callback = function()
						print("The user tapped Okay!")
					end
				},
			},
		})
	end,
})

local Button = TestTab:CreateButton({
	Name = "Universal Hub",
	Callback = function()
--		premium_key = "none";
		loadstring(game:HttpGet('https://zade.insane.rip/p/raw/5wf8db13y2'))()
	end,
})

